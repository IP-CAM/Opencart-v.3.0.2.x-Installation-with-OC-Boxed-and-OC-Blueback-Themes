<div id="content">
    <script type="text/javascript" src="view/javascript/supercheckout/js/jquery.tinysort.min.js"></script>
    <script type="text/javascript" src="view/javascript/jquery/jquery-ui/jquery-ui.js"></script>
    <link type="text/css" rel="stylesheet" href="view/javascript/jquery/jquery-ui/jquery-ui.css" >
    <style>
        .portlet {
            border: 1px solid #B5B5B5;
            clear: both;
            display: block;
            height: 90px;
            width: 95%;
            cursor: move;
            overflow: hidden;
            margin: 15px 10px;
        }
        
        .portlet-header {
            border-bottom: 1px solid #b5b5b5;
            text-align: center;
            font-size: 14px;
            font-weight: bold;
            color: #7C7C7C;
            min-width: 160px;
            margin: 5px 10px;
            padding-bottom: 5px;
        }
        
        .portlet-content .text {
            margin: 8px 10px 0px 10px;
            max-height: 33px;
            color: #7C7C7C;
            min-width: 160px;
            overflow: hidden;
            text-align: center;
        }
        
        .portlet-toggle {
            position: absolute;
            top: 50%;
            right: 0;
            margin-top: -8px;
        }
        
        .portlet-content {
            padding: 0.4em;
        }
        .portlet-placeholder {
            border: 1px dotted black;
            margin: 0 1em 1em 0;
            height: 50px;
        }
        .columns input {
            background: none;
            box-shadow: none;
            padding: 0px;
            height: 16px;
            text-align: center;
            border: none;
        }
        .sort {
            display: none;
        }
        .column {
            width: 180px;
            float: left;
            margin-bottom: 10px;
            margin-left: 0px;
            margin-right: 0px;
            margin-top: 10px;
            padding: 0px;
            /* height: 680px; */
            overflow: hidden;
        }
    </style>

    <div class="page-header">
        <div class="container-fluid">
            <div class="pull-right">
                <button type="submit" id="save-button" form="form-product" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
                <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
            </div>
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %} 
                    <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
                {% endfor %} 
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        {% if (error_warning) %} 
            <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }} 
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %} 
        {% if (success) %} 
            <div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }} 
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %} 
        <div id="error_message"></div>

